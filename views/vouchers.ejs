<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>WheelBazaar</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@v1.11.3/font/bootstrap-icons.css" rel="stylesheet">
    <link href="https://getbootstrap.com/docs/5.3/assets/css/docs.css" rel="stylesheet">
    <script src="/script/script.js"></script>
    <link rel="stylesheet" href="/CSS/style.css">

    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;700&display=swap" rel="stylesheet">
    <style>
        /* Style for the box */
        .box {
            width: 400px;
            height: 200px;
            background-color: #f8f9fa;
            border: 1px solid #dee2e6;
            border-radius: 10px;
            padding: 20px;
            margin: 50px auto;
            text-align: center;
            font-family: 'Poppins', sans-serif;
            font-size: 32px; /* Double the font size */
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease-in-out;
        }

        .box:hover {
            transform: scale(1.05);
        }

        /* Style for the larger font */
        .larger-font {
            font-size: 48px; /* Double the font size */
        }

        /* Style for the headline */
        .headline {
            margin-bottom: 20px; /* Increase margin for better spacing */
        }

        .float-container {
          padding: 20px;
        }
        
        .float-child {
            width: 50%;
            float: left;
            padding: 20px;
        } 

        .float-container {
          display: flex;
          flex-wrap: wrap;
        }

        .box {
          flex: 1 0 50%; /* Grow and shrink equally, base width 50% */
          max-width: 50%; /* Max width 50% */
        }
    </style>

    <style>
      #earningsChart {
          max-width: 500px;
          max-height: 300px;
          border: 2px dashed #000;
          padding: 10px;
          background-color: #f8f9fa;
          margin: 20px 0;
      }
  </style>

  <style>
    #myPieChart {
        max-width: 500px;
        max-height: 300px;
        padding: 10px;
        margin: 20px 0;
    }
  </style>

  </head>
  <body>
    <%-include('layouts/company_navbar.ejs', {user_info:company_info})%>
    <main>
        <div class="container">
            <div class="col-md-8">
                <h5 style="margin-left: 10%;font-style: italic;">Showing <span style="color: red;" id="lengthhh"><%=vouchers.length%></span> result(s)...</h5>
            </div>
        </div>
        <br>
        <div class="container" id="searchRes">
            <% for(var i=0;i<vouchers.length;i++) { %>
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title"><%=vouchers[i].VOUCHER_NAME%></h5>
                        <p class="card-text">Model name: <%=vouchers[i].MODEL_NAME%></p>
                        <p class="card-text">Color: <%=vouchers[i].COLOR%></p>
                        <p class="card-text">Discount: <%=vouchers[i].DISCOUNT%> %</p>
                        <p class="card-text">Validity date: <%=vouchers[i].VALIDITY_DATE%></p>
                        <!-- <button class="see-order-btn" data-info="<%=vouchers[i].SHOWROOM_ID %>" onclick="see_orders(this)">See Orders</button> -->
                    </div>
                </div>
            <%}%>
        </div>
    </main>
    <%-include('layouts/footer.ejs')%>


    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
  </body>
</html>

